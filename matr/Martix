package ru.vsuet.RogovT1;

import java.util.Random;
class Matrix {

    {
        int[][] m = new int[4][4];
        Random rnd = new Random();
        for (int i = 0; i < 4; i++) {
            for (int j = 0; j < 4; j++) {
                m[i][j] = rnd.nextInt(10) + 1;
            }
        }
    }
}
