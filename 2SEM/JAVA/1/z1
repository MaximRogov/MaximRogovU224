test
package ru.vsuet.Rogovt1;

import java.util.Random;

public class mtrx{
    private int n[][];
    public mtrx(int size){
        n = new int[size][size];
        Random rnd = new Random();
        for (int i =0;i < size;i++){
            for (int j = 0; j< size;i++){
                n [i][j] = rnd.nextInt(10)+1;
                System.out.print(n[i][j]);
            }
            System.out.print("\n");
        }
    }


}
