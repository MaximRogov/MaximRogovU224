#variant 5
from random import randint 


M=randint(1,5)
N=randint(1,5)

A = [[randint(0, 9) for i in range(M)] for _ in range(N)]
def D():
    
    
    for x in A:
        print(*x)
    print()
    for t in A:
        t = (list((1 if min(t) % 2 else 0) if j == min(t) else j for j in t))

        print(*t)
D()
