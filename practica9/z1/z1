#variant 5
def matr(a): 
  if len(a) <= 1:
    return a
  else:
    return matr( [x for x in a[1:] if x < a[0]])+[a[0]]+matr([x for x in a[1:] if x >= a[0]])
m=int(input('введите кол-во строк'))
n=int(input('введите кол-во стобцов'))
a = []
for i in range(m):
  b=[]
  for j in range(n):
    print('введите [',i,',',j,'] элемент')
    b.append(int(input()))
  a.append(b)
for i in range(m):
  for j in range(n):
    print(a[i][j],end=' ')
  print()
print(list(map(matr, a)))
